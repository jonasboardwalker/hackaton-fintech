{
  "name": "@admin-shad-template/web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "types": "./src/types/api/index.ts",
  "exports": {
    "./api": {
      "types": "./src/types/api/index.ts"
    }
  },
  "scripts": {
    "prebuild": "pnpm run generate-api-schema",
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "predev": "pnpm run generate-api-schema",
    "dev": "next dev -p 3000",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "generate-api-schema": "tsx scripts/generate-openapi.ts && npx openapi-typescript-codegen --input ./openapi.json --output ../../packages/api/src/generated --client axios"
  },
  "dependencies": {
    "@clerk/backend": "^1.24.2",
    "@clerk/localizations": "^3.10.7",
    "@clerk/nextjs": "^6.12.0",
    "@hookform/resolvers": "^4.1.1",
    "@prisma/client": "^6.4.1",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.66.9",
    "@tanstack/react-table": "^8.21.2",
    "@trpc/client": "11.0.0-rc.802",
    "@trpc/react-query": "11.0.0-rc.802",
    "@trpc/server": "11.0.0-rc.802",
    "@types/react-file-icon": "^1.0.4",
    "axios": "^1.7.9",
    "firebase": "^11.3.1",
    "form-data": "^4.0.2",
    "geist": "^1.3.1",
    "lucide-react": "^0.469.0",
    "next": "^15.1.7",
    "next-intl": "^3.6.0",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "react-file-icon": "^1.6.0",
    "react-hook-form": "^7.54.2",
    "react-responsive": "^10.0.0",
    "server-only": "^0.0.1",
    "superjson": "^2.2.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@admin-shad-template/eslint-config": "workspace:*",
    "@admin-shad-template/shared": "workspace:*",
    "@admin-shad-template/typescript-config": "workspace:*",
    "@admin-shad-template/ui": "workspace:*",
    "@tailwindcss/postcss": "^4.0.8",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.13.5",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.24.1",
    "@typescript-eslint/parser": "^8.24.1",
    "eslint": "^9.21.0",
    "eslint-config-next": "^15.1.7",
    "postcss": "^8.5.3",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "prisma": "^6.4.1",
    "tailwindcss": "^4.0.8",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "zod-openapi": "^4.2.4"
  },
  "ct3aMetadata": {
    "initVersion": "7.38.1"
  },
  "packageManager": "pnpm@9.15.2",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
